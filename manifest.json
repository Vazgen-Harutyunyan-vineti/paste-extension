{
  "name": "COI input helper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "COI values helper tool",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "activeTab",
    "https://*.apps.c3.core.vineti.com/ ; localhost"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.vineti.com/*",
        "http://localhost:3000/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}